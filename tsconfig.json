{
  "compileOnSave": true,
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "jsx": "react",
    "sourceMap": true,
    "removeComments": false,
    "noImplicitAny": false,
    "outDir": "_out"
  },
  "filesGlob": [
    "**/*.ts",
    "!node_modules/**",
    "!.c9/**",
    "!.git/**",
    "!typings/**"
  ],
  "exclude": [
    ".c9",
    ".git",
    "node_modules",
    "typings/main",
    "typings/main.d.ts"
  ]
}