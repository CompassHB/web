{
  "compileOnSave": true,
  "compilerOptions": {
    "jsx": "react",
    "lib": ["dom", "es5", "es2015", "es2015.core", "es2015.promise"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "outDir": "_out",
    "removeComments": false,
    "sourceMap": true,
    "strictNullChecks": true,
    "target": "es5"
  },
  "filesGlob": [
    "**/*.ts",
    "!node_modules/**",
    "!.c9/**",
    "!.git/**",
    "!typings/**"
  ],
  "exclude": [
    ".c9",
    ".git",
    "node_modules",
    "typings/main",
    "typings/main.d.ts"
  ]
}